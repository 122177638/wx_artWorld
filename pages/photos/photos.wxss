/* pages/photos/photos.wxss */
.addPhotosBox{
   padding:36rpx 20rpx 40rpx;
   display: flex;
   flex-wrap: wrap;
}
.addPhotos_btn{
   box-sizing: border-box;
   width:224rpx;
   height:224rpx;
   text-align: center;
   line-height: 224rpx;
   border:2rpx solid #d8d8d8;
   font-size:80rpx;
   color:#d8d8d8;
}
.photoBox{width:224rpx;height:224rpx;position: relative;margin-right:19rpx;margin-bottom:20rpx;}
.photoBox:nth-child(3n){margin-right:0;}
.photoImg{width:224rpx;height:224rpx;}
.delete_icon{width:54rpx;height:54rpx;position: absolute;right:10rpx;top:10rpx;}