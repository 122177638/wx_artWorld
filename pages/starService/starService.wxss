/* pages/starService/starService.wxss */
.starService_title{
   font-size:32rpx;
   color:#333333;
   text-align: center;
   padding-top:38rpx;
   padding-bottom:50rpx;
}
.star_show_content{padding:30rpx 20rpx;}
.star_show_item{display: flex;align-items: center;margin-bottom: 36rpx;}
.star_show_item:last-child{margin-bottom: 0rpx;}
.star_show_img{width:200rpx;height:238rpx;}
.star_show_bb{width:200rpx;height:238rpx;margin-right:30rpx;}
.star_show_right{width:450rpx;}
.show_right_occupation{
   margin-bottom:20rpx;
   display: flex;
   justify-content:space-between;
   font-size:30rpx;
   color:#000000;
}
.show_right_occupation .O_left{display: flex;}
.show_right_occupation .O_left view:nth-child(1){margin-right:58rpx;}
.O_right{color:#ED3439;}
.show_right_lis{font-size:24rpx;margin-bottom: 12rpx;color:#333333;}
.show_right_lis:nth-last-child(2){margin-bottom:12rpx;}
.show_right_lis text{word-wrap: break-word;word-break: break-all;}
.show_right_cost{display: flex;align-items: center;justify-content: flex-end;}
.show_right_cost text{font-size:30rpx;color:#000000;margin-right:94rpx;white-space: nowrap;}
.order_btn{
  width:180rpx;
  height:50rpx;
  font-size:26rpx;
  color:#ffffff;
  background-color:#ed3439;
  text-align:center;
  line-height:50rpx;
  border-radius: 70rpx;
}