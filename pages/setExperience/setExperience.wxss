/* pages/setDetails/setDetails.wxss */
.setGG{
  font-family: PingFangSC-Medium, sans-serif;
  padding-bottom: 130rpx;
}
.GGpoint{
  padding:28rpx 26rpx 0rpx;
  font-size:22rpx;
  color:#aeaeae;
  text-align: center;
}
.addItem{
  padding-top:40rpx;
  text-align: center;
}
.addItemBtn{
  width:710rpx;
  height:124rpx;
  vertical-align: middle;
}
.item{
  width:710rpx;
  height:188rpx;
  background-color:#FFFFFF;
  margin: 0 auto;
  margin-top:38rpx;
  padding:15rpx 40rpx 15rpx 60rpx;
  box-sizing: border-box;
  box-shadow:2rpx 2rpx 30rpx rgba(0, 0, 0, 0.7);
  position: relative;
}
.content{
  display: flex;
  
}
.importTxt{
  width:610rpx;
  height:158rpx;
  font-size:28rpx;
  color:#c9c9c9;
  word-break:break-all;
  word-wrap:break-word;
}
.PimportTxt{
  width:410rpx;
  height:158rpx;
  font-size:28rpx;
  color:#c9c9c9;
  word-break:break-all;
  word-wrap:break-word;
}
.addImg{
  width:180rpx;
  height:154rpx;
  padding-right:20rpx;
  display: inline-block;
}
.yidong{
  width:28rpx;height:22rpx;
  vertical-align: middle;
  position: absolute;
}
.shangyi{
  left:10rpx;
  top:40rpx;
}
.xiayi{
  left:10rpx;
  bottom:40rpx;
}
.delet{
  width:38rpx;
  height:38rpx;
  position: absolute;
  right:-10rpx;top:-10rpx;
}

.F_pot{
  position: fixed;
  left:0;
  bottom:0;
  width: 100%;
}
.btnBox{
  display: flex;
}
.yulan{
  width:50%;
  height:86rpx;
  background-color:#ffffff;
  color:#000000;
  font-size:34rpx;
  line-height: 86rpx;
  text-align: center;
}
.wancheng{
  width:100%;
  height:86rpx;
  background-color:#2ca940;
  font-size:34rpx;
  color:#ffffff;
  line-height: 86rpx;
  text-align: center;
}