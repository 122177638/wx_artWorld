/* pages/MyAnnouncement/MyAnnouncement.wxss */
.MyAnnouncement_container{
   font-family: PingFangSC-Medium, sans-serif;
}
.seachAnnouncement{
   display: flex;
   padding-top:28rpx;
   padding-bottom: 50rpx;
   justify-content: center;
}
.seachAnnouncement input{
   width:560rpx;
   height:52rpx;
   border-radius: 70rpx;
   border:2rpx solid #d8d8d8;
   font-size:28rpx;
   color:#989898;
   box-sizing: border-box;
   text-align: center;
}


.MyAnnouncements{margin-left:43rpx;margin-right:66rpx;border-left:2rpx solid #d8d8d8;}
.Announcement{margin-left:29rpx;position: relative;padding-bottom: 60rpx;}
.Announcement::before{
   content: '';
   width:30rpx;
   height:30rpx;
   display: inline-block;
   position: absolute;
   background:url('data:image/png;base64,') no-repeat;
   background-size:100% 100%;
   border-radius: 50%;
   top:0rpx;
   left:-44rpx;
}
.Announcement:nth-last-child(1)::after{
   content: '';
   width:30rpx;
   height:30rpx;
   display: inline-block;
   position: absolute;
   background:url('data:image/png;base64,') no-repeat;
   background-size:100% 100%;
   border-radius: 50%;
   bottom:0rpx;
   left:-44rpx;
}
.AnnouncementTitle{display: flex;justify-content: space-between;padding-bottom:16rpx;}
.AnnouncementTime{font-size:30rpx;color:#333333;}
.AnnouncementLeftTime{font-size:26rpx;color:#333333;}
.AnnouncementLeftTime text{color:#ee4247;}
.AnnouncementContent{background-color:#F3F3F3;border-radius: 10rpx;padding:0 20rpx;color:#333333;}
.AnnouncementNeed{
   font-size:28rpx;
   display: flex;
   justify-content: space-between;
   padding:24rpx 0;
   align-items: center;
}
.AnnouncementPoint{
   width:82rpx;
   height:34rpx;
   border-radius: 70rpx;
   border:2rpx solid #ed3439;
   text-align: center;
   line-height: 34rpx;
   color:#ed3439;
   font-size:24rpx;
}
.AnnouncementDetails{font-size:26rpx;padding-bottom: 20rpx;}
.AnnouncementAddress{font-size:24rpx;color:#707070;padding-bottom: 20rpx;}
.AnnouncementSpecificTime{font-size:24rpx;color:#707070;padding-bottom: 30rpx;}
.AnnouncementMoney{font-size:26rpx;color:#333333;padding-bottom: 30rpx;}