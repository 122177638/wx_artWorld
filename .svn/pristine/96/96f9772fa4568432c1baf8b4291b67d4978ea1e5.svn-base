/* pages/getStarMessage/getStarMessage.wxss */
.getStarMessage_item_title{
   width:100%;
   height:58rpx;
   background-color:#f3f3f3;
   padding:0 20rpx;
   font-size:30rpx;
   color:#333333;
   line-height: 58rpx;
}
.getStarMessage_item_title::before{
   content: '';
   width:10rpx;
   height:28rpx;
   display: inline-block;
   background-color:#000000;
   vertical-align: middle;
   margin-right:10rpx;
}
.getStarMessage_item_content{
   padding:20rpx 30rpx;
   font-size:28rpx;
   color:#333333;
}
.getStarMessage_item_content view{margin-bottom: 20rpx;}
