/* pages/Withdrawals/Withdrawals.wxss */
.Withdrawals_container{font-family: PingFangSC-Medium, sans-serif;}
.Withdrawals_top{padding:90rpx 0;text-align: center;}
.totalMoney{font-size:24rpx; color:#222222; display:inline-block;padding-bottom:28rpx;}
.totalMoney text{color:#ed3439;font-size:60rpx;font-weight: 600;}
.totalMoney_point{font-size:26rpx;color:#222222;}
.Withdrawals_center{margin:0 20rpx;border-bottom: 2rpx solid #f0f0f0;padding-bottom: 40rpx;}
.Withdrawals_import{padding-left:10rpx;padding-right:16rpx;display: flex;align-items: center;}
.MoneySize{font-size:48rpx;color:#222222;padding-right:50rpx;}
.importMoney{width:455rpx;border:none;outline: none;font-size:30rpx;color:#888888;}
.totalMoneyBtn{font-size:30rpx;color:#1f8ae3;}
.Withdrawals_point{padding:28rpx 30rpx 70rpx;font-size:20rpx;color:#888888;}
.Withdrawals_btn{
   width:690rpx;
   height:86rpx;
   font-size:32rpx;
   color:#ffffff;
   background-color:#f26d5f;
   border-radius: 10rpx;
   text-align: center;
   line-height:86rpx;
   margin:0 auto;
}
