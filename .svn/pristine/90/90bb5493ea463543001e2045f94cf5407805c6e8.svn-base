/* pages/setService/setService.wxss */
.Tpicker{
  font-size:32rpx;
  font-family: PingFangSC-Medium, sans-serif;
  color:#222222;
  background-color:#eeeeee;
  padding-bottom:90rpx;
}
.pickerss{
  padding:20rpx;
  border-bottom:2rpx solid #eeeeee;
  background-color:#ffffff;
}
.pickerList{
  padding:20rpx 40rpx;
  display: block;
}
.CityBox{
  position: fixed;
  width:500rpx;
  height:100%;
  top:0;
  right:0;
}
.cityHidden{
  display: none;
}
.cityShow{
  display: block;
  background-color:#d8d8d8;
}
.fontBtn{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  height:90rpx;
  background-color:green;
  line-height:90rpx;
  text-align: center;
  color:#ffffff;
}