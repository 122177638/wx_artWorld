/* pages/ggdetails/ggdetails.wxss */
.ggdetail{
  font-family: PingFangSC-Medium, sans-serif;
  background-color:#ffffff;
  display:flex;
  flex-direction: column;
  align-items: center;
}
.ggTop{
  font-size:24rpx;
  padding:20rpx 0;
  color:#ababab;
  width: 100%;
}
.userImg{
  width:64rpx;
  height:64rpx;
  border-radius: 50%;
  margin-left: 20rpx;
  vertical-align: middle;
}
.MsgDetails{
  width:95%;

}
.gsSketch{
  width: 100%;
  margin-bottom:50rpx;
  font-size:30rpx;
  color:#000000;}
.gsSketch2{
  width: 100%;
  margin-bottom:50rpx; 
  font-size:35rpx;
  color:#000000;
  display: flex;
  flex-direction:row;
}
.gsImgs{
  width:auto;
  height:auto;
}
.gsImg{
  width:100%;
}
.gslogos{
  width:100%;
  height:200rpx;
  margin-bottom: 40rpx;
  display: flex;
  flex-direction:row;
  justify-content:center;
}
.gslogo{
  border-radius: 128rpx;
  width:150rpx;
  height:150rpx;
}
.gslogos-li{
  font-size:30rpx;
  display: flex;
  flex-direction:column;
  justify-content:center;
  align-items: center;
}

