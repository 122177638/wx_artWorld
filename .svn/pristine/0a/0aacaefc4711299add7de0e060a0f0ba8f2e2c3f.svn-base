/* pages/Cashier/Cashier.wxss */
.Cashier_container{padding-bottom:100rpx;}
.Cashier_top{padding:30rpx 20rpx;border-bottom:20rpx solid #f3f3f3;}
.Cashier_msg_box{display: flex;align-items: center;padding-bottom:40rpx;}
.msg_head{width:66rpx;height:66rpx;border-radius: 50%;}
.Name{font-size:28rpx;color:#333333;margin-left:20rpx;}
.star_show_item{display: flex;align-items: center;}
.star_show_img{width:200rpx;height:238rpx;}
.star_show_bb{width:200rpx;height:238rpx;margin-right:30rpx;}
.star_show_right{width:450rpx;}
.show_right_occupation{margin-bottom:20rpx;display: flex;}
.show_right_occupation view{font-size:30rpx; color:#000000;}
.show_right_occupation view:nth-child(1){margin-right:98rpx;}
.show_right_lis{font-size:24rpx;margin-bottom: 12rpx;color:#333333;}
.show_right_lis:nth-last-child(2){margin-bottom:12rpx;}
.show_right_lis text{word-wrap: break-word;word-break: break-all;}
.show_right_cost{display: flex;align-items: center;justify-content:space-between;}
.show_right_cost text{font-size:30rpx;color:#000000;margin-right:94rpx;white-space: nowrap;}
.P_Num{font-size:30rpx;color:#333333;}

/* form  */
.form_item{border-bottom:2rpx solid #d8d8d8;padding:30rpx;display: flex;justify-content: space-between;align-items: center;}
.reduce,.plus{
   width:66rpx;
   height:66rpx;
   line-height: 66rpx;
   background-color:#f3f3f3;
   color:#333333;
   text-align: center;
   font-size:48rpx;
}
.Company{
   border:2rpx solid #d8d8d8;
   width:66rpx;
   height:66rpx;
   line-height: 66rpx;
   text-align: center;
   margin:0 30rpx;
}
.C_text{margin-left:20rpx;}
.item_left{font-size:32rpx;color:#333333;}
.item_right{display: flex;align-items: center;font-size:30rpx;}

.T_date{width:46rpx;height:46rpx;line-height: 46rpx;text-align: center;border:2rpx solid #ed3439;margin-left:30rpx;border-radius: 50%;color:#ed3439;}
.T_date1{width:46rpx;height:46rpx;line-height: 46rpx;text-align: center;border:2rpx solid #000000;margin-left:30rpx;border-radius: 50%;color:#000000;}
.on_T_date1{border-color:#ed3439;color:#ed3439;}
.item_right input{width:450rpx;text-align: right;}
.mg60{margin-left:60rpx;}
.liuyan{padding:30rpx;}
.liuyan_point{margin-bottom:30rpx;}
.liuyan_msg textarea{
   border:2rpx solid #d8d8d8;
   width:100%;
   height:166rpx;
   background-color:#f3f3f3;
   font-size:28rpx;
   color:#707070;
   padding:10rpx;
   box-sizing: border-box;
}
.form_ticket{border-bottom:2rpx solid #d8d8d8;}
.ticket_box{margin:0 30rpx;padding-bottom:30rpx;}
.ticket_list{background-color:#f3f3f3;padding:20rpx;font-size:28rpx;color:#707070;}
.agreement{text-align:center;font-size:28rpx;padding:20rpx;}
/* 脚 */
.foot_wrap{
   position: fixed;
   bottom:0;
   left:0;
   width:100%;
   display: flex;
   z-index: 99;
   background-color:#ffffff;
   font-size:36rpx;
   border-top:2rpx solid #f3f3f3;
   box-sizing: border-box;
}
.foot_left{
   width:460rpx;
   height:90rpx;
   line-height: 90rpx;
   text-align: center;
}
.F_inline{display: inline;}
.F_money{
   display: inline;
   color:#ed3439;
}
.foot_right{
   width:290rpx;
   height:90rpx;
   line-height: 90rpx;
   text-align: center;
   color:#ffffff;
   font-size:36rpx;
   background-color:#ed3439;
   border-radius: 0;
}

/* 弹窗  */
.tc_bg{
   width:100%;
   height:100%;
   background-color:rgba(0, 0, 0, 0.5);
   position: fixed;
   top:0;
   left:0;
}
.tc_container{
   position: absolute;
   width:610rpx;
   top:25%;
   left:50%;
   margin-left:-305rpx;
}
.tc_content{
   width:610rpx;
   background-color:#ffffff;
   border-radius: 15rpx;
}
.tc_title{
   padding:48rpx 0 40rpx;
   font-size:34rpx;
   font-weight: 600;
   color:#333333;
   text-align: center;
}
.tc_point_content{
   padding:50rpx 40rpx 50rpx;
   font-size:32rpx;
   color:#333333;
   text-align: center;
}
.tc_foot{
   display: flex;
   justify-content: space-between;
   padding-top:34rpx;
}
.tc_cancel_btn{
   width:290rpx;
   height:80rpx;
   line-height: 80rpx;
   background-color:#ffffff;
   text-align: center;
   border-radius: 15rpx;
   font-size:32rpx;
   color:#333333;
}
.tc_Determine_btn{
   width:290rpx;
   height:80rpx;
   line-height: 80rpx;
   background-color:#ed3439;
   text-align: center;
   border-radius: 15rpx;
   font-size:32rpx;
   color:#ffffff;
   margin:0;
   padding:0;
}
.tc_Determine_btn::after{border:none;}