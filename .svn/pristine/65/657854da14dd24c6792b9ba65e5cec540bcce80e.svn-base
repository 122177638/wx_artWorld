/* pages/setPhotos/setPhotos.wxss */
.setPhotos_container{font-family: PingFangSC-Medium, sans-serif;padding:30rpx 0;}
.p_point{text-align: center;font-size:32rpx;color:#333333;}
.setPhotos_top{
   width:702rpx;
   height:90rpx;
   border: 2rpx solid #d8d8d8;
   margin:0rpx auto 38rpx;
   font-size:32rpx;
   color:#d8d8d8;
   text-align: center;
   line-height: 90rpx;
}
.setPhotos_top::before{
   content: '+';
   font-size:32rpx;
   color:#d8d8d8;
   display: inline-block;
   margin-right:18rpx;
}
.setPhotos_list{
   padding:0 20rpx;
   display: flex;
   flex-wrap: wrap;
}
.setPhotos_item{
   width:338rpx;
   margin-right:34rpx;
   margin-bottom:34rpx;
}
.setPhotos_item:nth-child(2n){margin-right:0;}
.Photos_home{
   width:338rpx;
   height:338rpx;
   position: relative;
}
.Photos_homeImg{
   width:338rpx;
   height:338rpx;
   border-radius: 10rpx;
}
.delete_icon{
   width:54rpx;height:54rpx;
   position: absolute;
   right:10rpx;
   top:10rpx;
}
.Photos_title{font-size:28rpx;color:#333333;text-overflow:ellipsis;white-space:nowrap;overflow: hidden;margin:20rpx 0;}
.Photos_footnote{display: flex;font-size:24rpx;color:#707070;align-items: center;justify-content: space-between;}
.Photos_footnote view{display: flex;align-items: center;}
.Photos_footnote view view{width:140rpx;text-overflow:ellipsis;white-space:nowrap;overflow: hidden;display:block;}
.dingwei_icon{width:22rpx;height:26rpx;margin-right:10rpx;margin-left:28rpx;vertical-align: middle;}