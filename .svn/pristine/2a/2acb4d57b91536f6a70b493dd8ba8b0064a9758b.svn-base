<!--pages/getStarMessage/getStarMessage.wxml-->
<view class="getStarMessage_container">
   <view class="getStarMessage_item">
      <view class="getStarMessage_item_title">简介</view>
      <view class="getStarMessage_item_content">{{starMsg.starSketch}}</view>
   </view>
   <view class="getStarMessage_item">
      <view class="getStarMessage_item_title">资料</view>
      <view class="getStarMessage_item_content">
         <view>艺名： {{starMsg.starName}}</view>
         <view>性别： {{starMsg.starSex}}</view>
         <view>身高： {{starMsg.starHeight}}</view>
         <view>体重： {{starMsg.starWeight}}</view>
         <view>年龄： {{starMsg.starAge}}</view>
         <view>出生地： {{starMsg.starRegion}}</view>
         <view>职业： {{starMsg.starOccupation}}</view>
      </view>
   </view>
</view>
